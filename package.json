{
  "name": "@toy-box/header-bar",
  "version": "1.0.2",
  "author": "shopnb",
  "license": "MIT",
  "homepage": "@toy-box/header-bar",
  "repository": "https://github.com/toy-box/herder-bar.git",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish --access public --registry https://registry.npmjs.org",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "antlr4ts": "antlr4ts ./src/formula/FormulaLexer.g4 ./src/formula/FormulaParser.g4"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@airclass/icons": "^4.3.4",
    "@ant-design/icons": "^4.7.0",
    "@designable/core": "^1.0.0-beta.32",
    "@designable/shared": "^1.0.0-beta.32",
    "@formily/react": "^2.0.0-beta.50",
    "@formily/reactive": "^2.0.0-rc.20",
    "@formily/reactive-react": "^2.0.0-rc.20",
    "@toy-box/meta-schema": "^2.0.15",
    "@toy-box/toybox-shared": "^2.0.15",
    "@toy-box/toybox-ui": "^2.0.15",
    "@types/react": "^16.14.20",
    "@types/react-dom": "^16.9.14",
    "ahooks": "^2.10.12",
    "classnames": "^2.3.1",
    "codemirror": "^5.61.0",
    "express": "^4.17.1",
    "hoist-non-react-statics": "^3.3.2",
    "rc-motion": "^2.4.4",
    "rc-picker": "^2.5.19",
    "rc-table": "^7.19.0",
    "rc-util": "^5.14.0",
    "react-codemirror2": "^7.2.1"
  },
  "peerDependencies": {
    "antd": "^4.16.13",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@designable/core": "^1.0.0-beta.32",
    "@types/codemirror": "^5.60.0",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.14",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5",
    "yorkie": "^2.0.0"
  }
}
